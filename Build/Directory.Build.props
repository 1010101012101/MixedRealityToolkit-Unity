<Project>
    <PropertyGroup>
        <LibTargetFramework>netstandard2.0</LibTargetFramework>
        <MRTKRoot>..\..</MRTKRoot>
        <UnityInstallRoot>C:\Program Files\Unity</UnityInstallRoot>
        <UnityManagedRoot>$(UnityInstallRoot)\Editor\Data\Managed</UnityManagedRoot>
        <UnityEngineDll>$(UnityManagedRoot)\UnityEngine.dll</UnityEngineDll>
        <UnityEditorDll>$(UnityManagedRoot)\UnityEditor.dll</UnityEditorDll>
        <UnityEngineUIExtensionDll>$(UnityInstallRoot)\Editor\Data\UnityExtensions\Unity\GUISystem\UnityEngine.UI.dll</UnityEngineUIExtensionDll>
        <Version>1.0.0-alpha</Version>
        <OutputPath>$(MRTKRoot)\Build\__output</OutputPath>
        <MRTKConstants>UNITY_EDITOR;UNITY_2018_1_OR_NEWER</MRTKConstants>
        <DefineConstants>$(MRTKConstants)</DefineConstants>
    </PropertyGroup>
</Project>